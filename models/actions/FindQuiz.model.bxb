action (FindQuiz) {
  description (Finds the quiz the user specified. If no quiz specified, gives users all the quizzes so they can choose one.)
  type (Calculation)
  collect {
    input (searchTerm) {
      type (SearchTerm)
      min (Optional) max (One)
      description (Try to match a specific quiz)
    }
  }
  output (Quiz)
}
