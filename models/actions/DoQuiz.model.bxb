action (DoQuiz) {
  type (Commit)
  collect {
    input (quiz) {
      type (Quiz)
      min (Required) max (One)
      default-init {
        intent {
          goal: StartQuiz
        }
      }
    }
    input (answer) {
      type (Answer)
      min (Required) max (One)      
    }
  }
  output (Quiz)
}
